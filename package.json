{
  "name": "aftership",
  "description": "node.js SDK for AfterShip API",
  "version": "6.0.0-alpha",
  "homepage": "https://github.com/aftership/aftership-sdk-nodejs",
  "author": {
    "name": "AfterShip",
    "email": "support@aftership.com"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/aftership/aftership-sdk-nodejs.git"
  },
  "bugs": {
    "url": "https://github.com/aftership/aftership-sdk-nodejs/issues"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=10.0"
  },
  "scripts": {
    "prepare": "npm run build",
    "build": "npm run build:clean && tsc",
    "build:clean": "shx rm -rf ./dist ./coverage ./.nyc_output",
    "lint": "tslint --project .",
    "test": "nyc mocha -c --config .mocharc.json test/*.js",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov -t ${CODECOV_TOKEN} -F aftership"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/debug": "^4.1.5",
    "@types/node": "^13.11.1",
    "@types/uuid": "^7.0.2",
    "axios-mock-adapter": "^1.18.1",
    "chai": "^3.4.1",
    "codecov": "^3.0.4",
    "debug": "^4.1.1",
    "mocha": "^2.3.4",
    "nyc": "^14.1.1",
    "pre-commit": "^1.1.2",
    "shx": "^0.3.2",
    "sinon": "^1.17.2",
    "ts-node": "^8.2.0",
    "tslint": "^5.17.0",
    "tslint-config-airbnb": "^5.11.1",
    "typescript": "^3.5.1"
  },
  "keywords": [
    "aftership"
  ],
  "license": "MIT",
  "pre-commit": [
    "lint"
  ],
  "private": false
}
